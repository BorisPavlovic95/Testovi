@model List<Employee>

<table>
    <tr>
        <th>Name</th>
        <th>Total Time Worked</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr style="@(employee.TotalTimeWorked < 100 ? "background-color: #ff9999;" : "")">
            <td>@employee.EmployeeName</td>
            <td>@employee.TotalTimeWorked hours</td>
        </tr>
    }
</table>
